<odoo>
    <data noupdate="1">
        <record id="ir_cron_assign_sales_contribution_class" model="ir.cron">
            <field name="name">Assign Sales Contribution Class</field>
            <field name="model_id" ref="product.model_product_template"/>
            <field name="state">code</field>
            <field name="code">model._cron_assign_sales_contribution_class()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active">True</field>
        </record>
    </data>
    <data noupdate="1">
        <record id="ir_cron_calculate_ads" model="ir.cron">
            <field name="name">Calculate ADS</field>
            <field name="model_id" ref="product.model_product_template"/>
            <field name="state">code</field>
            <field name="code">model._cron_calculate_ads()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active">True</field>
        </record>
    </data>
</odoo>
