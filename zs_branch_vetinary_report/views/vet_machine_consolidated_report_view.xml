<odoo>
    <record id="view_consolidated_machine_report_form" model="ir.ui.view">
        <field name="name">vet.consolidated.machine.report.form</field>
        <field name="model">vet.consolidated.machine.report</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="report_date"/>
                        <field name="file_name" invisible="1"/>
                        <field name="file_data" filename="file_name" widget="binary"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_consolidated_machine_report_tree" model="ir.ui.view">
        <field name="name">vet.consolidated.machine.report.tree</field>
        <field name="model">vet.consolidated.machine.report</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="report_date"/>
                <field name="file_data" widget="binary" filename="file_name"/>
            </tree>
        </field>
    </record>

    <record id="action_consolidated_machine_report" model="ir.actions.act_window">
        <field name="name">Consolidated Reports</field>
        <field name="res_model">vet.consolidated.machine.report</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                No consolidated reports available yet!
            </p>
        </field>
    </record>

    <menuitem id="zs_vetinary_consolidated_report_view" name="Consolidated Veterinary Reports"
              action="action_consolidated_machine_report"
              parent="zs_inventory_planning_report.zs_head_scm_menu" sequence="8"/>
</odoo>