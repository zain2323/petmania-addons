<odoo>

        <record id="powerbi_dashboard_wizard_form_view" model="ir.ui.view">
            <field name="name">powerbi.dashboard.wizard.form.view</field>
            <field name="model">powerbi.dashboard.wizard</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Powerbi Dashboard">
                    <field name="powerbi_id"/>
                    <field name="name" invisible="1"/>
                    <div id="powerbi_dashboard_container" style="width:100%; height:500px;"></div>
                </form>
            </field>
        </record>

</odoo>