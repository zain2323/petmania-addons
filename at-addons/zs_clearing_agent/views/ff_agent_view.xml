<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="ff_agent_search_form_view" model="ir.ui.view">
        <field name="name">ff.agent.search.form</field>
        <field name="model">ff.agent</field>
        <field name="arch" type="xml">
            <search string="FF Agent">
                <field name="name" />
            </search>
        </field>
    </record>
    <record id="action_open_single_ff_agent" model="ir.actions.act_window">
        <field name="name">FF Agent</field>
        <field name="res_model">ff.agent</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="view_brand_tree" model="ir.ui.view">
        <field name="name">ff.agent.tree</field>
        <field name="model">ff.agent</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
            </tree>
        </field>
    </record>
    <record model="ir.actions.act_window" id="action_ff_agent">
        <field name="name">FF Agent</field>
        <field name="res_model">ff.agent</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem
        name="FF Agent"
        id="menu_ff_agent"
        action="action_ff_agent"
        parent="purchase.menu_purchase_config"
    />
</odoo>
