<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for Advance Reorder Process -->
        <record id="seq_advance_reorder_process" model="ir.sequence">
            <field name="name">Advance Reorder Process</field>
            <field name="code">advance.reorder.orderprocess</field>
            <field name="prefix">AR0</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for Advance Procurement Process -->
        <record id="seq_advance_procurement_process" model="ir.sequence">
            <field name="name">Advance Procurement Process</field>
            <field name="code">advance.procurement.process</field>
            <field name="prefix">AP0</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_forecast_product_sale" model="ir.sequence">
            <field name="name">Forecast Product Sale</field>
            <field name="code">forecast.product.sale</field>
            <field name="prefix">FS</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
