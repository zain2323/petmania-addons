<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <data>
        <record id="packing_nature_action" model="ir.actions.act_window">
            <field name="name">Packing Nature</field>
            <field name="res_model">packing.nature</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="packing_nature_view" model="ir.ui.view">
            <field name="name">Packing Nature</field>
            <field name="model">packing.nature</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <div class="oe_title">
                            <label for="name" string="Name"/>
                            <h1>
                                <field name="name"/>
                            </h1>
                        </div>
                    </sheet>
                </form>
            </field>
        </record>

         <menuitem id="packing_nature"
                   name="Packing Nature"
                   parent="sale.product_menu_catalog"
                   sequence="4"
                   action="packing_nature_action"/>
    </data>
</odoo>